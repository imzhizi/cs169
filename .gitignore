.DS_Store
.IDEA

# Ignore bundler config.
/hw3-rails-hangperson/.bundle


# Ignore the default SQLite database.
/hw3-rails-hangperson/db/*.sqlite3
/hw3-rails-hangperson/db/*.sqlite3-journal


# Ignore all logfiles and tempfiles.
/hw3-rails-hangperson/log/*
/hw3-rails-hangperson/tmp/*

!/hw3-rails-hangperson/log/.keep
!/hw3-rails-hangperson/tmp/.keep

# Ignore uploaded files in development
/hw3-rails-hangperson/storage/*
!/hw3-rails-hangperson/storage/.keep

/node_modules
/yarn-error.log

/public/assets
.byebug_history

# Ignore master key for decrypting credentials and more.
/hw3-rails-hangperson/config/master.key

